{
    "name": "defstudio/laravel-tools",
    "description": "A collection of laravel tools made by def:studio team",
    "type": "library",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "license": "MIT",
    "authors": [
        {
            "name": "Fabio Ivona",
            "email": "fabio.ivona@defstudio.it",
            "homepage": "https://defstudio.it",
            "role": "Developer"
        },
        {
            "name": "Daniele Romeo",
            "email": "danieleromeo@defstudio.it",
            "homepage": "https://defstudio.it",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "barryvdh/laravel-debugbar": "^3.6",
        "defstudio/blade-compiler": "dev-main",
        "defstudio/supercharged-carbon": "dev-master",
        "illuminate/support": "^8.0|^9.0|^10.0",
        "spatie/laravel-ray": "^1.25",
        "spatie/laravel-tail": "^4.4",
        "wire-elements/modal": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "DefStudio\\Tools\\": "src"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "DefStudio\\Tools\\ServiceProvider"
            ]
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    }
}
